{
  "extends": "../../tsconfig.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "baseUrl": ".",
    "plugins": [
      {
        "name": "typescript-styled-plugin",
        "tags": [
          "styled",
          "css"
        ]
      }
    ],
    "allowJs": true,
    "noEmit": true,
    "incremental": true
  }
}
